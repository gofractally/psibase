package psibase:component-parser;

interface intf {

    record imported-func {
        comp-intf: string,
        func-name: string,
    }
    
    record component-extraction {
        /// A list of imported functions
        imported-funcs : list<imported-func>,

        // The WIT definition of the component's world.
        wit: string
    }

    parse: func(name: string, bytes: list<u8>) -> result<component-extraction, string>;
}

world component {
    export intf;
}