package evaluations:plugin;


interface types {
    record evaluation {
        id: u32,
        rsvp-start-from: u32,
        rsvp-deadline: u32,
        evaluation-deadline: u32,
    }

}

interface api {
    use host:common/types.{error};
    use types.{evaluation};
    
    schedule-evaluation: func(rsvp-start-from: u32, rsvp-deadline: u32, evaluation-deadline: u32) -> result<_, error>;

}


world imports {
    import api;
}
