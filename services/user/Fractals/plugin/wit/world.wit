package fractals:plugin;

interface api {

    use host:common/types.{error};


    set-schedule: func(fractal: string, registration: u32, deliberation: u32, submission: u32, finish-by: u32, interval-seconds: u32, force-delete: bool) -> result<_, error>;
    create-fractal: func(account: string, name: string, mission: string) -> result<_, error>;

    join: func(fractal: string) -> result<_, error>;
}




world imports {
    import api;
}
